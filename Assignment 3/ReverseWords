#include <iostream>
using namespace std;
#include <vector>
#include <string>

string reverse(string str){

vector<char> reversed;
string temp = "";

for(int i = 0; i < str.length(); i++){
	if(str[i] == " "){
    	reverse.push_back(temp);
		temp = "";
    }
    else{
    	temp+=str[i];
    }
}

reversed.push_back(temp);
reverse(reversed.begin(), reversed.end());

string reversedString(reversed.begin(), reversed.end());
return reversedString;

}


