#include <iostream>
using namespace std;
#include <vector>
#include <string>

vector<string> binaryList(int num){

	int[10] temp;
    string str;
    vector<string> list;
    
    //given a number: n = 5
    //perform 5%2 -> 1 (first bit)
    //let n = n/2 -> 2
    //perform 2%2 -> 0 (second bit)
    
    while(num > -1){
    
    for(int i =0; num>0; i++){
    	temp[i] = num%2;
        num = num/2;
    }


	for(int j:temp){
    	str+= to_string(j);
    }
    
    list.push_back(str);
    num--;
    
    }
    
   reverse(list.begin(), list.end());
   return list;
}
